{
	"name": "prisma-class-generator",
	"version": "0.2.13",
	"description": "Class generator from Prisma schema",
	"main": "dist/index.js",
	"license": "MIT",
	"keywords": [
		"prisma",
		"nestjs",
		"typescript",
		"generator"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:AlgoLabsVN/prisma-class-generator.git"
	},
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\"",
		"dev": "node -r ts-node/register src/index.ts",
		"generate": "prisma generate",
		"build": "tsc"
	},
	"bin": {
		"prisma-class-generator": "dist/bin.js"
	},
	"engines": {
		"node": ">=18"
	},
	"author": {
		"name": "kimjbstar",
		"email": "kimjbstar@gmail.com"
	},
	"dependencies": {
		"@prisma/generator-helper": "^4.14.1",
		"@prisma/internals": "^4.14.1",
		"prettier": "2.5.1"
	},
	"devDependencies": {
		"@prisma/client": "4.14.1",
		"@types/jest": "^28.1.4",
		"@types/node": "^18.15.11",
		"@types/prettier": "^2.6.3",
		"prisma": "^4.14.1",
		"ts-node": "^10.9.1",
		"ts-toolbelt": "^9.6.0",
		"typescript": "^5.0.0"
	},
	"_moduleAliases": {
		"@src": "dist"
	}
}
